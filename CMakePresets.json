{
  "version": 7,
  "configurePresets": [
    {
      "name": "default",
      "displayName": "Default Config",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/out/soldat_${presetName}",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
      }
    },
    {
      "name": "default-clang",
      "inherits": "default",
      "displayName": "Clang Default",
      "cacheVariables": {
        "ENABLE_MOLD": "ON",
        "ENABLE_CLANG_TIDY": "ON",
        "CMAKE_C_COMPILER": "clang",
        "CMAKE_CXX_COMPILER": "clang++"
      }
    },
    {
      "name": "clang-asan",
      "displayName": "Clang Asan",
      "inherits": "default-clang",
      "cacheVariables": {
        "ENABLE_ASAN": "ON"
      }
    },
    {
      "name": "clang-coverage",
      "displayName": "Clang Coverage",
      "inherits": "default-clang",
      "cacheVariables": {
        "ENABLE_COVERAGE": "ON"
      }
    },
    {
      "name": "default-gcc",
      "inherits": "default",
      "displayName": "Gcc Default",
      "cacheVariables": {
        "CMAKE_C_COMPILER": "gcc",
        "CMAKE_CXX_COMPILER": "g++",
        "ENABLE_MOLD": "ON"
      }
    },
    {
      "name": "gcc-coverage",
      "displayName": "Gcc Coverage",
      "inherits": "default-gcc",
      "cacheVariables": {
        "ENABLE_COVERAGE": "ON"
      }
    },
    {
      "name": "gcc-asan",
      "displayName": "Gcc Asan",
      "inherits": "default-gcc",
      "cacheVariables": {
        "ENABLE_ASAN": "ON"
      }
    },
    {
      "name": "debug-emcc",
      "displayName": "Debug Emcc",
      "inherits": "default-clang",
      "cacheVariables": {
        "CMAKE_TOOLCHAIN_FILE": "/usr/lib/emscripten/cmake/Modules/Platform/Emscripten.cmake"
      }
    },
    {
      "name": "utbot_cpp",
      "displayName": "Utbot_cpp",
      "generator": "Unix Makefiles",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_C_COMPILER": "/utbot_distr/install/bin/clang",
        "CMAKE_CXX_COMPILER": "/utbot_distr/install/bin/clang++",
        "CMAKE_AR": "/utbot_distr/install/bin/llvm-ar",
        "CMAKE_LINKER": "/utbot_distr/install/bin/llvm-link",
        "CMAKE_NM": "/utbot_distr/install/bin/llvm-nm",
        "CMAKE_OBJDUMP": "/utbot_distr/install/bin/llvm-objdump",
        "CMAKE_RUNLIB": "/utbot_distr/install/bin/llvm-ranlib",
        "CMAKE_OBJCOPY": "/utbot_distr/install/bin/llvm-objcopy",
        "CMAKE_EXPORT_LINK_COMMANDS": "ON",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "default",
      "hidden": true,
      "displayName": "Default Build",
      "configurePreset": "default",
      "configuration": "Release",
      "targets": [
        "SoldatGame",
        "SoldatGame_full"
      ]
    },
    {
      "name": "clang-debug",
      "displayName": "Clang Debug",
      "inherits": "default",
      "configurePreset": "default-clang",
      "configuration": "Debug"
    },
    {
      "name": "clang-debug-asan",
      "displayName": "Clang Debug Asan",
      "inherits": "default",
      "configurePreset": "clang-asan",
      "configuration": "Debug"
    },
    {
      "name": "clang-debug-coverage",
      "displayName": "Clang Debug Coverage",
      "inherits": "default",
      "configurePreset": "clang-coverage",
      "configuration": "Debug"
    },
    {
      "name": "gcc-debug",
      "displayName": "Gcc Debug",
      "inherits": "default",
      "configurePreset": "default-gcc",
      "configuration": "Debug"
    },
    {
      "name": "gcc-debug-coverage",
      "displayName": "Gcc Debug Coverage",
      "inherits": "default",
      "configurePreset": "gcc-coverage",
      "configuration": "Debug"
    },
    {
      "name": "gcc-debug-asan",
      "displayName": "Gcc Debug Asan",
      "inherits": "default",
      "configurePreset": "gcc-asan",
      "configuration": "Debug"
    }
  ]
}